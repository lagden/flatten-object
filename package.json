{
  "name": "@tadashi/flatten-object",
  "version": "3.1.0",
  "description": "Flattens object a single level deep",
  "type": "module",
  "main": "src/flatten.js",
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "c8 --reporter=text --reporter=lcov ava"
  },
  "keywords": [
    "flatten",
    "object",
    "level",
    "deep"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "c8": "7.10.0",
    "timekeeper": "2.2.0",
    "xo": "0.46.4"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "ignore": [
      "bin/**/*.*"
    ],
    "semicolon": false,
    "rules": {
      "padding-line-between-statements": 0,
      "unicorn/prevent-abbreviations": 0,
      "capitalized-comments": 0
    }
  }
}
