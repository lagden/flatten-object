{
  "name": "@tadashi/flatten-object",
  "version": "2.0.0",
  "description": "Flattens object a single level deep",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava"
  },
  "keywords": [
    "flatten",
    "object",
    "level",
    "deep"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.1"
  },
  "devDependencies": {
    "ava": "3.5.0",
    "xo": "0.28.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "padding-line-between-statements": 0,
      "unicorn/prevent-abbreviations": 0,
      "capitalized-comments": 0
    }
  }
